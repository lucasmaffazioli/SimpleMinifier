{
	"name": "simpleminify",
	"displayName": "Simple Minifier",
	"description": "Simply remove enters and double spaces from selected text.",
	"version": "1.0.3",
	"publisher": "LucasMaffazioli",
	"engines": {
		"vscode": "^1.36.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/lucasmaffazioli/SimpleMinifier"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onCommand:extension.simpleminify"
	],
	"icon": "images/icon.png",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.simpleminify",
				"title": "Simple minifier"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0"
	}
}